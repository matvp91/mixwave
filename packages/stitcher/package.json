{
  "name": "@mixwave/stitcher",
  "type": "module",
  "exports": {
    "./schemas": "./dist/src/schemas.js"
  },
  "typesVersions": {
    "*": {
      "schemas": [
        "src/schemas.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "node './dist/src/index.js'",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/index.js\""
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/parse-filepath": "^1.0.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/http-proxy": "^9.5.0",
    "@zodyac/env": "^1.1.8",
    "fastify": "^4.28.1",
    "parse-filepath": "^1.0.2",
    "persistent-node-cache": "^1.2.0",
    "zod": "^3.23.8"
  }
}
